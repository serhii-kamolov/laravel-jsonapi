{
    "name": "serhiikamolov/laravel-jsonapi",
    "description": "A set of interfaces and classes to facilitate the construction of an efficient Json Api application with the Laravel framework.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Serhii Kamolov",
            "email": "serhii.kamolov@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "illuminate/http": "^7.0|^8",
        "illuminate/database": "^7.0|^8",
        "illuminate/pagination": "^7.0|^8",
        "illuminate/support": "^7.0|^8",
        "illuminate/routing": "^7.0|^8",
        "illuminate/auth": "^7.0|^8",
        "illuminate/testing": "^7.0|^8"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "JsonAPI\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JsonAPI\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "mockery/mockery": "^1.3"
    }
}
